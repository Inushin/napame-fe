/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "node"
    ],
    "paths": {
      "@app/*": ["./src/app/*"],
      "@base/*": ["./src/app/base/*"],
      "@environments/*": ["./src/environments/*"],
      "@interceptors/*": ["./src/app/interceptors/*"],
      "@pages/*": ["./src/app/pages/*"],
      "@services/*": ["./src/app/services/*"],
    }
  },
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
